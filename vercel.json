{
  "version": 2,
  "builds": [
    {
      "src": "api/[...path].py",
      "use": "@vercel/python"
    },
    {
      "src": "web/**",
      "use": "@vercel/static"
    }
  ],
  "functions": {
    "api/[...path].py": {
      "maxDuration": 60
    }
  },
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/web/$1" }
  ]
}
